{
  "name": "facebook-ad-landing-system",
  "version": "1.0.0",
  "description": "广告投放落地页管理系统，集成Facebook广告审核屏蔽功能（cloak）和域名管理功能",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "deploy": "chmod +x deploy.sh && ./deploy.sh"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.6.3",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.5.0",
    "geoip-lite": "^1.4.7",
    "user-agents": "^1.1.32",
    "express-useragent": "^1.0.15",
    "moment": "^2.29.4",
    "sharp": "^0.32.6",
    "dotenv": "^16.3.1",
    "archiver": "^6.0.1",
    "unzipper": "^0.10.14",
    "winston": "^3.11.0",
    "cron": "^3.1.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "facebook",
    "ads",
    "landing-page",
    "cloak",
    "domain-management"
  ],
  "author": "Landing Page System",
  "license": "MIT"
}