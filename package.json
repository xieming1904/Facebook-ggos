{
  "name": "facebook-ad-landing-system",
  "version": "1.0.0",
  "description": "广告投放落地页管理系统，集成Facebook广告审核屏蔽功能（cloak）和域名管理功能",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "deploy": "chmod +x deploy.sh && ./deploy.sh",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:ci": "NODE_ENV=test jest --ci --coverage --watchAll=false",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\" \"tests/**/*.js\"",
    "type-check": "tsc --noEmit",
    "docs": "node src/scripts/generateDocs.js",
    "healthcheck": "node src/scripts/healthcheck.js",
    "migration": "node src/scripts/migration.js",
    "seed": "node src/scripts/seedDatabase.js",
    "backup": "node src/scripts/backup.js",
    "analyze": "node src/scripts/analyze.js",
    "precommit": "npm run lint && npm run format:check && npm run test:ci"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.6.3",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.5.0",
    "geoip-lite": "^1.4.7",
    "user-agents": "^1.1.32",
    "express-useragent": "^1.0.15",
    "moment": "^2.29.4",
    "sharp": "^0.32.6",
    "dotenv": "^16.3.1",
    "archiver": "^6.0.1",
    "unzipper": "^0.10.14",
    "winston": "^3.11.0",
    "cron": "^3.1.6",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "@slack/web-api": "^6.11.2",
    "mongoose-paginate-v2": "^1.7.4",
    "ws": "^8.14.2",
    "redis": "^4.6.8",
    "bull": "^4.11.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "@types/node": "^20.8.9",
    "@types/express": "^4.17.21",
    "@types/mongoose": "^5.11.97",
    "@types/jest": "^29.5.7",
    "@types/bcryptjs": "^2.4.5",
    "@types/jsonwebtoken": "^9.0.5",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "mongodb-memory-server": "^9.1.1",
    "babel-jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.0.3",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3"
  },
  "keywords": [
    "facebook",
    "ads",
    "landing-page",
    "cloak",
    "domain-management"
  ],
  "author": "Landing Page System",
  "license": "MIT"
}